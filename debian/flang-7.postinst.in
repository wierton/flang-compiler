#! /bin/sh -e

update-alternatives --install /usr/bin/f95 f95 /usr/bin/flang 10 \
  --slave /usr/lib/@ARCH@/fortran/default flibdir /usr/lib/@ARCH@/fortran/flang-7
  #--slave /usr/share/man/man1/f95.1.gz f95.1.gz /usr/share/man/man1/gfortran.1.gz

update-alternatives --install /usr/bin/f77 f77 /usr/bin/flang 10 \
  #--slave /usr/share/man/man1/f77.1.gz f77.1.gz /usr/share/man/man1/gfortran.1.gz



exit 0
